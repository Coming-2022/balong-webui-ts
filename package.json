{
  "name": "mt5700m-balong-webui",
  "version": "1.0.0",
  "description": "MT5700M 5G模组Web管理界面 - TypeScript版本",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:sequential": "node scripts/start-sequential.js",
    "dev:delayed": "node scripts/start-delayed.js",
    "server:dev": "nodemon --exec ts-node src/server/index.ts",
    "client:dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "start:prod": "NODE_ENV=production node dist/server/index.js",
    "start:single": "node scripts/start-single-port.js",
    "docker:build": "./scripts/docker-build.sh",
    "docker:build:force": "./scripts/docker-build.sh --force",
    "docker:build:simple": "./scripts/docker-build.sh --simple",
    "docker:start": "./scripts/docker-manage.sh start",
    "docker:stop": "./scripts/docker-manage.sh stop",
    "docker:restart": "./scripts/docker-manage.sh restart",
    "docker:logs": "./scripts/docker-manage.sh logs",
    "docker:update": "./scripts/docker-manage.sh update",
    "upgrade:check": "npm outdated",
    "upgrade:safe": "./scripts/upgrade-deps.sh safe",
    "upgrade:toolchain": "./scripts/upgrade-deps.sh toolchain",
    "upgrade:status": "./scripts/upgrade-deps.sh status",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf dist logs node_modules/.cache"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^11.1.0",
    "winston": "^3.10.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^7.0.2"
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "MT5700M",
    "5G",
    "WebUI",
    "TypeScript",
    "React",
    "AT命令"
  ],
  "author": "MT5700M WebUI Team",
  "license": "MIT"
}
